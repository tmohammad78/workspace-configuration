{
  "explorer.confirmDelete": true,
  "css.lint.duplicateProperties": "warning",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.argumentsInColorFunction": "warning",
  "typescript.validate.enable": true,
  "editor.fontLigatures": true,
  "editor.fontFamily": "'DankMono Nerd Font Mono', 'Alibaba'",
  "terminal.integrated.fontFamily": "'DankMono Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1,
  "editor.lineHeight": 23,
  "editor.fontSize": 16,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "vscode-icons",
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#ff1a1a",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "?",
      "color": "#0066ff",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "backgroundColor": "transparent"
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "backgroundColor": "transparent"
    },
    {
      "tag": "*",
      "color": "#0ac231",
      "strikethrough": false,
      "backgroundColor": "transparent"
    }
  ],
  "codesnap.showWindowControls": false,
  "codesnap.backgroundColor": "#f2f2f2",
  "editor.semanticTokenColorCustomizations": {
    "enabled": true
  },
  "editor.tokenColorCustomizations": {
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "trailing",
  "workbench.sideBar.location": "left",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "editor.dragAndDrop": false,
  "editor.tabSize": 2,
  "workbench.colorTheme": "Dracula Soft",
  "codesnap.containerPadding": "0",
  "codesnap.transparentBackground": true,
  "vetur.validation.style": false,
  "workbench.startupEditor": "none",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "vue": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.tabs.enabled": false,
  "diffEditor.renderSideBySide": false,
  "files.autoSave": "afterDelay",
  "vetur.useWorkspaceDependencies": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "window.zoomLevel": 2
}
